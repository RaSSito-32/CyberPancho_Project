<?xml version="1.0" ?>
<robot name="e-puck">
    <webots>
        <device reference = "ps5" type="E-puckDistanceSensor">
            <ros>
                <topicName>/left_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <device reference="ps7" type="E-puckDistanceSensor">
            <ros>
                <topicName>/front_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <plugin type="my_package.cyberPancho_driver.MyRobotDriver" />
    </webots>
</robot>


