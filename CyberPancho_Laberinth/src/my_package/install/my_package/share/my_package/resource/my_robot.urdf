<?xml version="1.0" ?>
<robot name="My robot">
    <webots>
        <device reference="ps5" type="DistanceSensor">
            <ros>
                <topicName>/left_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <device reference="ps7" type="DistanceSensor">
            <ros>
                <topicName>/front_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <plugin type="my_package.cyberPancho_driver.MyRobotDriver" />
    </webots>
</robot>


